# Git branch function
parse_git_branch() {
    git branch 2>/dev/null | sed -e '/^[^*]/d' -e 's/* \(.*\)/ \1/'
}

# Git status with beautiful icons
parse_git_status() {
    local git_status="$(git status 2>/dev/null)"
    local git_dirty=""
    
    [[ $git_status =~ "Changes not staged" ]] && git_dirty="%F{yellow}âš¡%f"
    [[ $git_status =~ "Changes to be committed" ]] && git_dirty="${git_dirty}%F{green}âœ“%f"
    [[ $git_status =~ "Untracked files" ]] && git_dirty="${git_dirty}%F{red}â—%f"
    
    echo $git_dirty
}

# Complete git info
git_prompt_info() {
    local branch=$(parse_git_branch)
    if [ -n "$branch" ]; then
        local status=$(parse_git_status)
        echo "%F{white}â”‚%F{magenta}%B git%b%F{white}::%F{cyan}${branch}%f${status} "
    fi
}

# Background jobs with icon
job_count() {
    local job_n=$(jobs | wc -l)
    if [ $job_n -gt 0 ]; then
        echo "%F{yellow}âš™ $job_n%f "
    fi
}

# Exit code indicator
exit_code() {
    echo "%(?.%F{green}â—.%F{red}âœ˜ %?)%f"
}

# Colored path
colored_path() {
    echo "%F{cyan}%(5~|%-1~/â€¦/%3~|%~)%f"
}

# Time with icon
current_time() {
    echo "%F{blue}ğŸ• %*%f"
}

# User with conditional color
colored_user() {
    if [[ $EUID -eq 0 ]]; then
        echo "%F{red}%Bâš¡ %n%b%f"
    else
        echo "%F{green}%BğŸ‘¤ %n%b%f"
    fi
}

# Hostname
colored_host() {
    echo "%F{magenta}%BğŸ–¥  %m%b%f"
}

# Prompt symbol
prompt_symbol() {
    if [[ $EUID -eq 0 ]]; then
        echo "%F{red}%Bâ¯â¯â¯%b%f"
    else
        echo "%F{green}%Bâ¯%b%f"
    fi
}

