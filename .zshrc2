# Base64 encode/decode
b64encode() {
    [ -z "$1" ] && echo "Usage: b64encode <string>" && return 1
    echo -n "$1" | base64
}

b64decode() {
    [ -z "$1" ] && echo "Usage: b64decode <base64_string>" && return 1
    echo -n "$1" | base64 -d
}

