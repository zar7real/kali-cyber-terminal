# Compress function
compress() {
    [ -z "$1" ] || [ -z "$2" ] && echo "Usage: compress <archive.ext> <file/dir>" && return 1
    
    case "$1" in
        *.tar.gz)  tar czf "$1" "$2" ;;
        *.tar.bz2) tar cjf "$1" "$2" ;;
        *.zip)     zip -r "$1" "$2"  ;;
        *.7z)      7z a "$1" "$2"    ;;
        *)         echo "Unsupported: use .tar.gz .tar.bz2 .zip .7z" ;;
    esac
    
    [ $? -eq 0 ] && echo -e "\e[32mâœ“ Created: $1\e[0m"
}

